@model List<GenreDTO>

@{
    ViewData["Title"] = "Index";
}


<div class="col-6">
    <h1>Genres</h1>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var genre in Model)
            {
                <tr>
                    <td>@genre.ID</td>
                    <td>@genre.Name</td>
                </tr>
            }
        </tbody>
    </table>
</div>